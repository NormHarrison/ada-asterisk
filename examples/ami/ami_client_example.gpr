with "../../ada_asterisk.gpr";

project ami_client_example is

   for Main use ("ami_client_example");
   for Source_Dirs use ("src");
   for Object_Dir use "obj";
   for Exec_Dir use "bin";

   package Compiler is
      for Switches ("Ada") use
        ("-gnatwae",
         "-g");
   end Compiler;

   package Binder is
      for Switches ("Ada") use
        ("-E");
   end Binder;

end ami_client_example;
